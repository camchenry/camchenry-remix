{
  "private": true,
  "name": "camchenry-remix",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup",
    "build": "npm run build:css && remix build",
    "dev": "remix run",
    "dev:netlify": "cross-env NODE_ENV=development netlify dev",
    "start": "remix-serve netlify/functions/server/build",
    "lint": "eslint --ext js,jsx,ts,tsx app/**",
    "lint:fix": "eslint --ext js,jsx,ts,tsx --fix app/**",
    "watch:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production"
  },
  "dependencies": {
    "@netlify/functions": "^0.7.2",
    "@remix-run/netlify": "^0.18.1",
    "@remix-run/node": "^0.18.1",
    "@remix-run/react": "^0.18.1",
    "@remix-run/serve": "^0.18.1",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "canvas": "2.6.1",
    "date-fns": "^2.21.3",
    "date-fns-tz": "^1.1.4",
    "highlight.js": "^10.7.2",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^6.0.0-beta.0",
    "remark": "^13.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "remark-parse-frontmatter": "^1.0.3",
    "remix": "0.17.0",
    "tailwind-styled-components": "^2.0.2",
    "tailwindcss": "^2.1.2",
    "unified": "^9.2.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^0.18.1",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "14"
  },
  "sideEffects": false
}
